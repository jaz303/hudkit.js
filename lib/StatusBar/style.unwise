.hk-status-bar {
    background: #595959;
    border-top: 1px solid #7D7D7D;
    box-sizing: border-box;
    padding: 0 5px;
}

.hk-status-bar .left-cells {
    float: left;
}

.hk-status-bar .right-cells {
    float: right;
}

.hk-status-bar .cell {
    float: left;
    height: 20px;
}

.hk-status-bar .cell.text {
    color: white;
    font-size: 11px;
    font-family: Helvetica;
    padding-top: 3px;
    text-shadow: #202020 0 -1px 1px;
}